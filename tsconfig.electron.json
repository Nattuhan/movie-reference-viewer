{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "outDir": "electron-dist",
    "rootDir": "electron",
    "baseUrl": ".",
    "declaration": true,
    "composite": true
  },
  "include": ["electron/**/*"],
  "exclude": ["node_modules"]
}
